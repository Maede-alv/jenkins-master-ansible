jenkins_version: "2.516.1"
java_package: "openjdk-17-jdk"
jenkins_plugins_file: "files/plugins.txt"
jenkins_admin_username: "admin"
jenkins_admin_password: "your-secret-password"  # Replace or use Ansible Vault
